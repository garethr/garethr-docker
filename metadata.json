{
  "name": "garethr-docker",
  "version": "4.1.0",
  "author": "Gareth Rushgrove",
  "summary": "Module for installing and managing docker",
  "license": "Apache-2.0",
  "source": "https://github.com/garethr/garethr-docker.git",
  "project_page": "https://github.com/garethr/garethr-docker",
  "issues_url": "https://github.com/garethr/garethr-docker/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },{
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "6.6"
      ]
    },{
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },{
      "operatingsystem": "Archlinux"
    },{
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "21"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.7"
    },
    {
      "name": "puppet",
      "version_requirement": "3.4"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.1.0"
    },{
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.1.0 <= 3.0.0"
    },{
      "name": "stahnma/epel",
      "version_requirement": ">= 0.0.6"
    }
  ]
}
